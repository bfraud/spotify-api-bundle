{
  "name": "bfraud/spotify-api-bundle",
  "description": "A Symfony bundle that provides integration for the bfraud/spotify-web-api-php-sdk lib",
  "authors": [
    {
      "name": "Benjamin Fraud",
      "email": "benjamin.fraud@gmail.com"
    }
  ],
  "require": {
    "bfraud/spotify-web-api-php-sdk": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7.7"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/bfraud/spotify-web-api-php-sdk.git"
    }
  ],
  "autoload": {
    "psr-4": { "SpotifyAPIBundle\\": ["src/"] }
  },
  "minimum-stability": "dev"
}
